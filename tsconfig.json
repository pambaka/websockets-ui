{
    "compilerOptions": {
        /* Language and Environment */
        "target": "ES2022",
        /* Modules */
        "module": "Node16",
        "moduleResolution": "node16",
        /* Interop Constraints */
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        /* Type Checking */
        "strict": true,
        "noImplicitAny": true,
        /* Completeness */
        "skipLibCheck": true
    }
}
